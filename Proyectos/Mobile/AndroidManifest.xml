<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coming.cellprojectmanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".actividades.MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>        
        <activity
            android:name=".actividades.ActualizarProgresoTarea"
            android:label="@string/title_activity_actualizar_progreso_tarea"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".actividades.SeleccionarSitio"
            android:label="@string/title_activity_seleccionar_sitio"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".actividades.SeleccionarEstado"
            android:label="@string/title_activity_seleccionar_estado"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>